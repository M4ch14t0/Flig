# ========================================
# CONFIGURAÇÕES DE PRODUÇÃO - FRONTEND FLIG
# ========================================
# 
# IMPORTANTE: Este arquivo contém configurações para PRODUÇÃO
# NUNCA commite este arquivo com valores reais!
# Use apenas como referência e configure no servidor de produção
#
# @author Flig Team
# @version 1.0.0

# ========================================
# CONFIGURAÇÕES DA API
# ========================================
# URL da API em produção
VITE_API_URL=https://api.flig.com.br

# ========================================
# CONFIGURAÇÕES DE DESENVOLVIMENTO
# ========================================
# Desabilitar modo de desenvolvimento
VITE_DEV_MODE=false

# Desabilitar autenticação mock
VITE_USE_MOCK_AUTH=false

# ========================================
# CONFIGURAÇÕES DE DOMÍNIO
# ========================================
# URL do frontend em produção
VITE_FRONTEND_URL=https://app.flig.com.br

# ========================================
# CONFIGURAÇÕES DE ANALYTICS
# ========================================
# Google Analytics
VITE_GA_TRACKING_ID=GA-XXXXXXXXX

# ========================================
# CONFIGURAÇÕES DE MONITORAMENTO
# ========================================
# Sentry (para monitoramento de erros)
VITE_SENTRY_DSN=your-sentry-dsn

# ========================================
# CONFIGURAÇÕES DE PAGAMENTO
# ========================================
# Mercado Pago
VITE_MERCADOPAGO_PUBLIC_KEY=your-mercadopago-public-key

# PagSeguro
VITE_PAGSEGURO_EMAIL=your-pagseguro-email

# ========================================
# CONFIGURAÇÕES DE NOTIFICAÇÕES
# ========================================
# Firebase (para push notifications)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
VITE_FIREBASE_APP_ID=your-firebase-app-id

# ========================================
# CONFIGURAÇÕES DE MAPAS
# ========================================
# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# ========================================
# CONFIGURAÇÕES DE SEO
# ========================================
# Meta tags
VITE_APP_TITLE=Flig - Filas Virtuais Inteligentes
VITE_APP_DESCRIPTION=Sistema de filas virtuais para estabelecimentos
VITE_APP_KEYWORDS=filas,virtuais,estabelecimentos,espera

# ========================================
# CONFIGURAÇÕES DE PERFORMANCE
# ========================================
# Cache
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600

# ========================================
# CONFIGURAÇÕES DE SEGURANÇA
# ========================================
# Content Security Policy
VITE_CSP_ENABLED=true

# ========================================
# CONFIGURAÇÕES DE DEBUG
# ========================================
# Logs de debug
VITE_DEBUG_ENABLED=false
VITE_LOG_LEVEL=error
